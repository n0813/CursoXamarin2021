<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="MiPrimeraAppEnXamarinForms.Pages.Modelo">
    <ContentPage.ToolbarItems>
        <ToolbarItem x:Name="toolbarRegistrar"   IconImageSource="ic_add_xam.png"
                     Clicked="toolbarRegistrar_Clicked" Priority="1" />
        <ToolbarItem x:Name="toolbarVer"   IconImageSource="ic_eyes.png"
                     Clicked="toolbarVer_Clicked" Priority="0" />
       
    </ContentPage.ToolbarItems>

    <ContentPage.Content>
        <StackLayout>
            <Label Text="Welcome to Xamarin.Forms!"
                VerticalOptions="CenterAndExpand" 
                   
                HorizontalOptions="CenterAndExpand" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>